{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "temp-tauri-app",
  "version": "0.1.0",
  "identifier": "com.temp-tauri-app.app",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1421",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist"
  },
  "app": {
    "macOSPrivateApi": true,
    "windows": [
      {
        "title": "",
        "titleBarStyle": "Overlay",
        "width": 1350,
        "height": 800,
        "minWidth": 440,
        "minHeight": 700
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
